first_comment_is_literal: true
#max_prefix_chars: 10
additional_commands:
  file:
    kwargs:
      GLOB: '*'
      GLOB_RECURSE: '*'
      STRINGS: '*'
      REGEX: '*'
  gtest_discover_tests:
    flags:
      - NO_PRETTY_TYPES
      - NO_PRETTY_TYPES
    kwargs:
      EXTRA_ARGS: '*'
      TEST_PREFIX: '*'
      TEST_SUFFIX: '*'
      PROPERTIES: '*'
      TEST_LIST: '*'
      DISCOVERY_TIMEOUT: '*'
      WORKING_DIRECTORY: '*'
  find_package_handle_standard_args:
    kwargs:
      REQUIRED_VARS: '*'
      VERSION_VAR: '*'
  target_include_directories:
    pargs: 1
    flags:
      - SYSTEM
      - BEFORE
    kwargs:
      INTERFACE: '*'
      PUBLIC: '*'
      PRIVATE: '*'
